{
  "name": "mercurius-typegraphql-prisma-starter",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "preserve": "npm run build",
    "serve": "cross-env NODE_ENV=development concurrently \"tsc --watch\" \"nodemon -q dist/index.js\"",
    "watch": "tsc -w",
    "dev:watch": "nodemon dist/index.js",
    "dev": "ts-node-dev --transpile-only --respawn --no-notify ./src/index.ts",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "db:init": "npx prisma init",
    "db:generate": "npx prisma generate",
    "db:migrate:new": "npx prisma migrate dev",
    "db:migrate:up": "npx prisma migrate"
  },
  "keywords": [],
  "author": "Vincent Abesamis (https://github.com/abevince)",
  "license": "MIT",
  "devDependencies": {
    "@types/ioredis": "^4.22.3",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "concurrently": "^6.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "fastify-tsconfig": "^1.0.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "prisma": "^2.21.0",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@prisma/client": "^2.21.0",
    "argon2": "^0.27.2",
    "fastify": "^3.14.2",
    "fastify-cookie": "^5.3.0",
    "fastify-cors": "^5.2.0",
    "fastify-sensible": "^3.1.1",
    "graphql": "^15.5.0",
    "ioredis": "^4.26.0",
    "mercurius": "^7.4.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1"
  }
}
